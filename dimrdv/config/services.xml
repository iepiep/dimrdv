<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="Iepiep\Dimrdv\Service\ItineraryService">
        </service>

        <service id="Iepiep\Dimrdv\Controller\Admin\AdminDimrdvItineraryController" public="true">
            <argument type="service" id="Iepiep\Dimrdv\Service\ItineraryService" />
            <argument>%dimrdv.google_api_key%</argument>
        </service>

        <service id="Iepiep\Dimrdv\Controller\Admin\AdminDimrdvConfigController" public="true">
        </service>

        <service id="Iepiep\Dimrdv\Controller\Admin\AdminDimrdvGestionRdvController" public="true">
        </service>
    </services>

    <parameters>
        <parameter key="dimrdv.google_api_key">%env(DIMRDV_GOOGLE_API_KEY)%</parameter>
    </parameters>
</container>
